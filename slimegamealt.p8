--goober
goober = {
sprite = 64,
x = 22*8,
y = 12*8,
dy = 0,
dx = -1,
jump_height = 0,
gravity = .3,
gravity_max = 8,
grounded = false,
timer = 0,
start_time = 0,

update = function(self)
    --run
    self:run()

    --gravity
    if not self.grounded then
        self.dy += self.gravity
        if self.dy > self.gravity_max then
            self.dy = self.gravity_max
        end
        --adjust y accordingly
        self.y += self.dy
    end

    --check grounded
    self.grounded = on_floor(self)

    --if in floor, get out
    while on_floor(self) do
        self.y -= 1
    end
end,

draw = function(self)
    --colliding
    if colliding(self, player) then
        cursor((self.x-player.x)+64, self.y-8)
        print("eEP!",7)
        cursor(0,0)
        self.sprite = 80
        self.timer = 1
        self.start_time = time()
    elseif time() > self.start_time + self.timer then
        self.timer = 0
    end

    --walks
    if not colliding(self, player) and (time() % 1 ) < .5 then
        self.sprite = 64
    elseif not colliding(self, player) then
        self.sprite = 65
    end

    spr(self.sprite, (self.x-player.x)+64, self.y+1, 1,1,(self.dx < 1))
end,

run = function(self)
    if not colliding(self, player) then
        self.x += self.dx
        if map_collision(self,7) then
            --flip direction
            self.dx *= -1
            while map_collision(self) do
                --get out of wall
                self.x += self.dx
            end
            self.x += self.dx
        end
    end
end
}


