update = function(self)
    --animate
    self:sprite_set()

    --jump
    self:jump()

    --gravity
    if not self.grounded then
        self.dy += self.gravity
        if self.dy > self.gravity_max then
            self.dy = self.gravity_max
        end
    end

    --adjust y accordingly
    self.y += self.dy

    --check grounded
    self.grounded = on_floor(self)

    --if in floor, get out
    while on_floor(self) do
        self.y -= 1
    end
end,

draw = function(self)
    if colliding(self, player) then
        cursor((self.x-player.x)+64, self.y-8)
        print("hELLO!",7)
        cursor(0,0)
    end
    spr(self.sprite, (self.x-player.x)+64, self.y+1, 1,1,(player.x < self.x))
end,

jump = function(self)
    seconds_between_jumps = 3
    if time()%seconds_between_jumps == 0 then
        self.dy = -self.jump_height
    end
end,

sprite_set = function(self)
    if colliding(self, player) then
        self.sprite = 83
        self.timer = 1
        self.start_time = time()
    elseif time() > self.start_time + self.timer then
        self.timer = 0
        self.sprite = 84
    end
    if not self.grounded and abs(self.dy) < .3 then
        self.sprite = 84
   	elseif not self.grounded and self.dy < 0 then
        self.sprite = 68
    elseif not self.grounded and self.dy > 0 then
        self.sprite = 67
    end
end,
