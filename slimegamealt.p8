--squirter
squirter = {
x = 0,
y = 0,
globs = {},

update = function(self)
	self.x = player.x+3
    self.y = player.y+7
	foreach(self.globs, function(obj) obj:update_glob() end)
end,

draw = function (self)
	foreach(self.globs, function(obj) obj:draw() end)
end,

squirt = function(self, upcount, uppies, downcount, downies, lifespan)
    i = 0
    globs = upcount
    while i < globs do
        self:create_glob(uppies,lifespan)
        i += 1
    end
    i = 0
    globs = downcount
    while i < globs do
        self:create_glob(downies,lifespan)
        i += 1
    end
end, 

create_glob = function(self, uppies, lifespan)
	new_glob = {
        x = squirter.x + (rnd()-.5)*3,
        y = squirter.y,
        dy = (rnd())-uppies,
        dx = (rnd()-.5),
        burnout = lifespan +  flr(rnd(8)),

        update_glob = function(self)
            self.x += self.dx
            while (fget(mget(flr(self.x)/8,self.y/8), 7)) and self.dx < 0 do
                self.x = flr(self.x)
                self.x += 1
                self.dx = 0
            end
            while (fget(mget(ceil(self.x)/8,self.y/8), 7)) and self.dx > 0 do
                self.x = ceil(self.x)
                self.x -= 1
                self.dx = 0
            end

            self.y += self.dy/2
            while (fget(mget(self.x/8,self.y/8), 7)) do
                self.y -= 1
                self.dx = 0
            end
            while (fget(mget(self.x/8,self.y/8), 6)) do
                self.y -= 1
                self.dx = 0
            end

            self.dy +=0.2
            self.burnout-=1
            if self.burnout < 0 then
                del(squirter.globs, self)
            end
        end,   

        draw = function(self)
            pset((ceil(self.x)-player.x+65), self.y, 12 )
        end
    }
	add(self.globs, new_glob)
end
}
