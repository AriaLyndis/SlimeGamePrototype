--main
function _init()
    pressed = false
    --pal(1, 129,1)
    run_delay = 0
    run_frame = 0
    turnaround_frame = 0
    turnaround = false
    splat = false
    splat_frame = 0
    bounce_splat = false
end

function _update()
    frame_count = flr(time()*30)
	player:update()
	goober:update()
    spoober:update()
    squirter:update()
    bubbler:update()
    --slimeter:update()
end

function _draw()
	cls()
	spr(48, player.x/-128+10,5)
	--map(player.x/8-8, player.y/8-10, -(player.x%8)+1, -(player.y%8), 18, 18)
    map(player.x/8-8, 0, -(player.x%8)+1, 0, 18, 18)
    goober:draw()
    spoober:draw()
    squirter:draw()
    bubbler:draw()
    player:draw()
    --slimeter:draw()
    print(flr(player.x),7)
    print(flr(player.y),7)
end

function debug_menu()
    print(player.x.." x")
    print(player.y.." y")
    print(tostr(player.grounded).." grounded")
end

function colliding(obj1, obj2)
    right1 = obj1.x + 7
	left1 = obj1.x
	top1 = obj1.y 
	bottom1 = obj1.y + 7

    right2 = obj2.x + 7
	left2 = obj2.x
	top2 = obj2.y 
	bottom2 = obj2.y + 7
    return (((right1 >= left2) and (right1 <= right2))or ((left1 <= right2) and (left1 >= left2))) and ((bottom1 >= top2) and top1 <= bottom2)
end

function animation_sprite(sprite_set, run_frame, frame_duration)
    sprite = sprite_set[flr(((run_frame / frame_duration)  % #sprite_set))+1]
    return sprite
end

function map_collision(obj, flag)
	right = obj.x + 7
	left = obj.x
	top = obj.y 
	bottom = obj.y + 7
    --solids
    if flag == 7 then --if solid
        if (fget(mget(flr(left/8),flr(bottom/8)), flag) or fget(mget(flr(right/8),flr(bottom/8)), flag)) then
            return true
        end
        if (fget(mget(flr(left/8),flr(top/8)), flag) or fget(mget(flr(right/8),flr(top/8)), flag)) then
            return true
        end
    end

    --one way patforms
    if flag == 6 then
        if (fget(mget(flr(left/8),flr(bottom/8)), flag) or fget(mget(flr(right/8),flr(bottom/8)), flag)) then
            return true
        end 
    end

    --water
    if flag == 4 then --if solid
        if (fget(mget(flr(left/8),flr(bottom/8)), flag) or fget(mget(flr(right/8),flr(bottom/8)), flag)) then
            return true
        end
        if (fget(mget(flr(left/8),flr(top/8)), flag) or fget(mget(flr(right/8),flr(top/8)), flag)) then
            return true
        end
    end
end

function on_floor(obj)
    bottom = obj.y + 8
    right = obj.x + 7
	left = obj.x
    if (fget(mget(flr(left/8),flr(bottom/8)), 7) or fget(mget(flr(right/8),flr(bottom/8)), 7)) then
		return true
    elseif (fget(mget(flr(left/8),flr(bottom/8)), 6) or fget(mget(flr(right/8),flr(bottom/8)), 6)) and obj.dy >=0 then
		return true
	end

end

function btn_press(button)
    if btnp(button) and not pressed then
        pressed = true
        return true
    elseif  not btn(button) then
        pressed = false
    end
end
