--player
player = {
    --Image
    sprite = 9,
    run_frame = 0,
    flipped = false,
    animation,
    --Movement
    x = 64,
    y = 64,
    dy = 0,
    dx = 0,
    acceleration = .15,
    friction = .5, --change into a stat for terrain?
    max_speed = 2.5,
    jump_height = -4.3,
    gravity = .45,
    gravity_max = 7,
    boost_speed = 3,
    --States
    flipped = false,
    free = true,   
    grounded = false,
    boosting = false,
    double_jump_ready = false,
    bounce_ready = false,

    update = function(self)
        if(self.free) then
            self:run(self.run_direction())
            self:double_jump()
            self:jump()
            self:bounce()
            self:check_grounded()
        end
    end,
    
    draw = function(self)
        self:acid_form()
        self:update_sprite()
        if map_collision(self, 4) then
            ovalfill(62, self.y-1, 73, self.y+9, 1)
        end
        spr(self.sprite, 64, self.y,1,1, self.flipped)
        if map_collision(self, 4) then
            oval(62, self.y-1, 73, self.y+9, 7)
        end
    end,

    update_sprite = function(self)
        --idle sprite
        self.sprite=9
        --turnaround test
        if (btn(‚û°Ô∏è) and self.flipped and not btn(‚¨ÖÔ∏è)) or (btn(‚¨ÖÔ∏è) and not self.flipped and not btn(‚û°Ô∏è)) then
            turnaround = true
        end
        --run sprite
        if self.dx < 0 then
            self.flipped = true
        elseif self.dx> 0 then 
            self.flipped = false
        end
        --expression sprite
        if btn(üÖæÔ∏è) and btn(‚¨áÔ∏è) then
    	    player.sprite = 26
        elseif btn(üÖæÔ∏è) then
            player.sprite = 25  
        end
        --air sprite
        if  not self.grounded and self.dy < 0 then
            self.sprite = 11
        elseif not self.grounded and self.dy > 0 then
            self.sprite = 12
        end
        --turnaround
        if turnaround then
            self.sprite = 26
            turnaround_frame += 1
        end
        if turnaround_frame >= 5 then
            turnaround_frame = 0
            turnaround = false
        end
        --roll
        if (btn(‚û°Ô∏è) and player.free and player.grounded and turnaround_frame == 0 and not btn(‚¨ÖÔ∏è)) then
            run_frame += 1
            player.sprite = animation_sprite({41,42,43,44,45,9}, run_frame, 3)
        elseif (btn(‚¨ÖÔ∏è) and player.free and player.grounded and turnaround_frame == 0 and not btn(‚û°Ô∏è)) then
            run_frame += 1
            player.sprite = animation_sprite({41,42,43,44,45,9}, run_frame, 3   )
        else
            --run_frame = 0
        end
        --confusion
        if btn(‚û°Ô∏è) and btn(‚¨ÖÔ∏è) then
            self.sprite = 26
        end
        
        --splat!
        if bounce_splat then
            rect(63, self.y+6, 72, self.y+7,12) 
            self.sprite = 27
            bounce_splat = false
            splat = false
            splating = false
        end
        if splat or splatting and not bounce_splat then
            splatting = true
            rect(63, self.y+6, 72, self.y+7,12)  
            self.sprite = 27
            splat_frame += 1
        end
        if splat_frame >= 4 then
            splat_frame = 0
            splatting = false
            self.sprite = 10
        end
        
    end,

    run_direction = function()  
        --if right not left
        if (btn(‚û°Ô∏è) and not btn(‚¨ÖÔ∏è)) then    
            return "right"
        --if left not right
        elseif btn(‚¨ÖÔ∏è) and not btn(‚û°Ô∏è) then 
            return "left"
        --if left <--> right
        else
            return nil
        end
    end,

    run = function(self, direction)
        if (direction == "right") then
            self.dx += self.acceleration
            if (self.dx >= self.max_speed) then
                self.dx -= self.acceleration
            elseif (self.dx <= 0) then
                self.dx += self.acceleration
            end
        elseif (direction == "left") then
            self.dx -= self.acceleration
            if (self.dx <= -(self.max_speed)) then
                self.dx += self.acceleration
            elseif (self.dx >= 0) then
                self.dx -= self.acceleration
            end
        else
            --Reduce dx by friction amount if above 0
            if self.dx > 0 then
                self.dx -= self.friction
                --If reduced past 0, set to 0
                if self.dx < 0 then
                    self.dx = 0
                end
            --Increase dx by friction amount if below 0
            elseif self.dx < 0 then
                self.dx += self.friction
                --If increased past 0, set to 0
                if self.dx > 0 then
                    self.dx = 0
                end
            end
        end
        self.x += self.dx
		while map_collision(player, 7) and self.dx <= 0 do
			self.x +=1
		end
        while map_collision(player, 7) and self.dx >= 0 do
			self.x -=1
		end
    end,

    check_grounded = function(self)
        if on_floor(self) and not self.grounded then
            if self.bounce_ready then
                sfx(4,1)
                bounce_splat = true
            else
                if self.dy > 7 then
                    sfx(1,1)
                end
                splat = true
            end
        else 
            splat = false    
        end
        if on_floor(self) and self.bounce_ready then
            self.dy *= -1
            self.bounce_ready = false
            self.double_jump_ready = true 
        elseif on_floor(self) then
            self.grounded = true
            slimeter.level = 3
            self.dy = 0
            self.double_jump_ready = true
            self.friction = 0.5
        else
            self.grounded = false
            self.friction = 0.2
        end
    end,

    jump = function(self)
        --take input and affect variables
        if (btnp(‚¨ÜÔ∏è) and self.grounded) then
            sfx(0,1)
            squirter:squirt(5,2,20,1,40)
            self.dy = self.jump_height
            self.double_jump_ready = true
        elseif (btn(‚¨ÜÔ∏è) and  not self.grounded and self.dy < 0 and self.dy > self.jump_height) then
            self.dy += self.gravity
        elseif (not self.grounded) then
            self.dy += self.gravity * 2
        end

        --check gravity max
        if self.dy > self.gravity_max then
            self.dy = self.gravity_max
        end
        --adjust y accordingly
        self.y += self.dy

        --shunt
        --solids
        while map_collision(player, 7) and self.dy >= 0 do
			self.y -=1
		end
        while map_collision(player, 7) and self.dy <= 0 do
			self.y +=1
		end
        --platforms
        while map_collision(player, 6) and self.dy >= 0 do
			self.y -=1
		end

    end,

    double_jump = function(self)
        if (btnp(‚¨ÜÔ∏è) and self.double_jump_ready) then
            sfx(2,1)
            squirter:squirt(10,.5,25,-.5,5)
            self.dy = self.jump_height
            self.double_jump_ready = false
            self.bounce_ready = false
        end
    end,

    bounce = function(self)
        if (btn_press(üÖæÔ∏è) and not self.grounded) then
            sfx(3,1)
            self.dy = self.gravity_max
            self.bounce_ready = true
        end
    end,

    float = function(self)
        --blah
    end,

    acid_form = function(self)
        if btn(‚ùé) then 
            pal(12,11)
            if rnd() < .3 then 
                bubbler:create_bubble()
            end
        else
            pal (12, 12)
        end
    end

