create_bubble = function(self)
	new_bubble = {
        x = bubbler.x -4 + flr((rnd())*8),
        y = bubbler.y-2,
        dy = .4,
        dx = (rnd()-.5)/3,
        burnout = 10 + flr(rnd(15)),

        update_bubble = function(self)
            self.y -= self.dy
            self.x -= self.dx
            self.burnout-=1
            if self.burnout < 0 then
                del(bubbler.bubbles, self)
            end
        end,   

        draw = function(self)
            pset((ceil(self.x)-player.x+65), self.y, 11 )
        end
    }
	add(self.bubbles, new_bubble)
end
